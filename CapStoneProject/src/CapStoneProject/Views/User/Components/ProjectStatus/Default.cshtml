@model List<Project>

<div class="container" id="formDiv">
    <div class="row">
        <br />
        <table class="table table-striped table-bordered table-condensed">
            <tr>           
                <th>Project Name</th>
                <th>Status</th>
                <th>Date</th>
            </tr>
            @foreach (Project p in Model)
            {
                <tr>
                    <td>@p.ProjectName</td>
                    <td>@p.ProjectStatus</td>
                    <td>@p.StatusDate</td>
                </tr>
            }
            
        </table>
    </div>
</div>

